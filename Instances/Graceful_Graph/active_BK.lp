%% Extra Predicates
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
lessthan(X,Y) :- num(X), num(Y), X <Y.
lessthan(X,Y) :- node(X), node(Y), X <Y.
%lessthan(edge(X1,Y1),edge(X2,Y2)) :- edge(X1,Y1), edge(X2,Y2), X1 <X2.
%lessthan(edge(X,Y1),edge(X,Y2)) :- edge(X,Y1), edge(X,Y2), Y1 <Y2.
next(X,Y) :- clique(X,Z), clique(Y,Z+1).
maxC(X) :- next(_,X), not next(X,_).
minC(X) :- clique(X,1).
